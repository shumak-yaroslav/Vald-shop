    android:id="@+id/nav_graph24"
    app:startDestination="@id/First24Fragment">

    <fragment
        android:id="@+id/First24Fragment"
        android:name="com.example.androidshop.First24Fragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first24">

        <action
            android:id="@+id/action_First24Fragment_to_Second24Fragment"
            app:destination="@id/Second24Fragment" />
